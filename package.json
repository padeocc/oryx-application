{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "algolia": "tsx src/algolia/indexation.ts",
    "static": "next build && next export",
    "typeorm": "ts-node -P ./src/db/tsconfig.json ./node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm migration:create ./src/db/migrations/ManualMigration",
    "migration:generate": "npm run typeorm migration:generate ./src/db/migrations/AutoMigration -- -d src/db/dataSource.ts",
    "migration:run": "npm run typeorm migration:run -- -d src/db/dataSource.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/db/dataSource.ts"
  },
  "dependencies": {
    "@contentstack/json-rte-serializer": "^3.0.0",
    "@mantine/core": "7.14.1",
    "@mantine/form": "7.14.1",
    "@mantine/notifications": "7.14.1",
    "@mantine/spotlight": "7.14.1",
    "@phosphor-icons/react": "^2.1.7",
    "algoliasearch": "^5.22.0",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "next": "15.2.4",
    "next-intl": "^4.0.2",
    "nextjs-cors": "^2.2.0",
    "nodemailer": "^6.10.0",
    "pg": "^8.16.3",
    "qs": "^6.14.0",
    "react": "19.0.0",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "19.0.0",
    "react-google-recaptcha": "^3.1.0",
    "reflect-metadata": "^0.2.2",
    "supertokens-auth-react": "^0.49.1",
    "supertokens-node": "^22.1.0",
    "supertokens-web-js": "^0.15.0",
    "typeorm": "^0.3.27",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.16",
    "@types/node": "^22",
    "@types/nodemailer": "^6.4.17",
    "@types/qs": "^6.9.18",
    "@types/react": "^19.0.12",
    "@types/react-google-recaptcha": "^2.1.9",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5"
  },
  "overrides": {
    "nextjs-cors": {
      "next": "15.2.4"
    }
  }
}
